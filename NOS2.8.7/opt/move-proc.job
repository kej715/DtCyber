~eor
.PROC,MOVPROC,FN,UI.
$ATTACH,FN/NA.
$IF,FILE(FN,AS),DA.
$  PURGE,FN.
$  SUI,UI.
$  PURGE,FN/NA.
$  DEFINE,F=FN${1}.
$  COPY,FN,F.
$ELSE,DA.
$  GET,FN.
$  PURGE,FN.
$  SUI,UI.
$  PURGE,FN/NA.
$  SAVE,FN${1}.
$ENDIF,DA.
$NOTE,OUTPUT,NR./1*** MOVE COMPLETE
$REVERT,NOLIST.
EXIT.
$NOTE,OUTPUT,NR./1*** MOVE FAILED
$REVERT,ABORT.
